{
    "name": "rails-nav",
    "displayName": "rails-nav",
    "description": "Rails navigation",
    "version": "0.0.1",
    "publisher": "jemmyw",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.navigateRails"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.navigateRails",
                "title": "Rails Navigation"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/fs-extra": "^4.0.2",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typescript": "^2.5.3",
        "vscode": "^1.1.5"
    },
    "dependencies": {
        "fs-extra": "^4.0.2"
    }
}
